{
  "name": "project",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  },
  "scripts": {
    "lint": "yarn eslint . --cache --fix --ext js,jsx,ts,tsx && :",
    "tsc": "yarn lerna run tsc && :",
    "start:client": "yarn lerna run start:client",
    "build:client": "yarn lerna run build:client",
    "storybook": "start-storybook -p 6006",
    "fontello": "bash ./fontello.sh",
    "fontello:open": "fontello-cli --config packages/libs/assets/fonts/fontello/config.json open",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ."
  },
  "engines": {
    "node": "<=14.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint",
      "yarn tsc"
    ]
  },
  "dependencies": {
    "@project/libs": "0.0.1",
    "@solana/spl-token": "^0.1.8",
    "@solana/wallet-adapter-base": "^0.7.0",
    "@solana/wallet-adapter-phantom": "^0.7.0",
    "@solana/wallet-adapter-wallets": "^0.11.3",
    "@solana/web3.js": "^1.31.0",
    "add": "^2.0.6",
    "antd": "^4.17.4",
    "axios": "^0.21.1",
    "classnames": "2.2.6",
    "connected-react-router": "6.8.0",
    "date-fns": "2.0.1",
    "formik": "2.2.6",
    "history": "4.10.0",
    "node-sass": "6.0.1",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll": "1.8.4",
    "react-toastify": "7.0.3",
    "redux": "4.1.2",
    "redux-persist": "6.0.0",
    "redux-saga": "1.1.3",
    "yarn": "^1.22.17",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-viewport": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/classnames": "2.2.10",
    "@types/history": "4.7.8",
    "@types/node": "16.11.7",
    "@types/ramda": "0.27.48",
    "@types/react": "17.0.34",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.3.2",
    "@types/react-scroll": "1.8.3",
    "@types/redux-saga": "0.10.5",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "^8.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fontello-cli": "0.6.2",
    "husky": "4.3.0",
    "lerna": "4.0.0",
